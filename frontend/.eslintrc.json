{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "comma-dangle": "off",
        "padded-blocks": "off",
        "arrow-body-style": "off",
        "linebreak-style": 0,
        "react/jsx-indent-props": 0,
        "react/jsx-indent": 0,
        "react/react-in-jsx-scope": "off",
        "indent": "off",
        "jsx-quotes": 0,
        "import/no-unresolved": "off",
        "react/no-children-prop": 0
    }
}
